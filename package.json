{
    "name": "preview_3d",
    "devDependencies": {
        "cross-env": "^7.0.3",
        "electron": "^23.0.0-beta.5",
        "electron-builder": "^23.6.0",
        "electron-packager": "^17.1.1"
    },
    "scripts": {
        "prebuild": "node prebuild.js",
        "start-sample": "npm run prebuild && cross-env NODE_ENV=development electron . assets/samples/Teapot.glb",
